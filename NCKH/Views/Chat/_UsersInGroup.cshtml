@model IEnumerable<NCKH.Models.ItemAllUser>

    @foreach (var user in Model)
    {
	<div href="#" class="list-group-item list-group-item-action border-0 dark-backg white-text">
			<div class="d-flex align-items-start">
				<img src="~/ImgChat/user.png" class="rounded-circle mr-1" alt="Sharon Lessman" width="40" height="40">
				<div class="flex-grow-1 ml-3">
					@user.Email
					<div class="small"><span class="fas fa-circle chat-online"></span> Online</div>
				</div>
			</div>
		</div>
    }
